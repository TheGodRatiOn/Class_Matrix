#include <iostream>
#include <cmath>

using namespace std;

int main()
{
	int n;
	cin >> n;

	int * PoiterToArray = new int[10];
	int Counter = 0;

	for (int i = 0; i < 10; i++)
	{
		PoiterToArray[i] = (i + 2) * ((n % 3) + 4) % 500 * -1 * (i + 2 - n);
	}


	__asm
	{
		mov ebx, PoiterToArray
		mov ecx, 10
		mov edx, n

		ArrayLoop :
		cmp edx, 0
			jng IncCounter

			dec edx
			mov eax, [ebx]
			neg eax
			mov[ebx], eax

			IncCounter :
		          cmp[ebx], 0
			  jnl lp
			  inc Counter
			  lp :

		  add ebx, type int
	       loop ArrayLoop
	}
	
	for (int i = 0; i < 10; i++)
	{
		cout << PoiterToArray[i] << " ";
	}

	cout << Counter << endl;

	delete PoiterToArray;
	system("pause");

	return 0;
}
